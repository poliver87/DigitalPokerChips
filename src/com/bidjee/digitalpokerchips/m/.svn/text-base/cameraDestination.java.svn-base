package com.bidjee.digitalpokerchips.m;

public class ZoomDestination {
	
	private float x;
	private float y;
	float zoomFactor;
	
	public ZoomDestination() {
		zoomFactor=1;
	}
	
	public void set(float x_,float y_,float zoomFactor_) {
		x=x_;
		y=y_;
		zoomFactor=zoomFactor_;
	}

	public float getY() {
		return y;
	}

	public float getX() {
		return x;
	}

	public float getZoomFactor() {
		return zoomFactor;
	}

}
